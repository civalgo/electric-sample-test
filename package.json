{
  "name": "electric-sql-expo-example",
  "version": "0.7.0",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "backend:start": "node ./backend/startCompose.js",
    "backend:stop": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml stop",
    "backend:up": "yarn backend:start --detach",
    "backend:down": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml down --volumes",
    "client:generate": "yarn electric:check && npx electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/electric",
    "client:watch": "yarn client:generate --watch",
    "db:migrate": "node ./db/migrate.js",
    "db:psql": "node ./db/connect.js",
    "db:electify": "node ./db/electify-tables.js",
    "electric:check": "node ./check-electric-is-running.js",
    "ports:configure": "node ./change-ports.js",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "civalgo:migrate-prisma": "yarn prisma migrate dev --schema db/schema.prisma",
    "civalgo:reset": "yarn backend:down && rimraf src/generated && yarn backend:up",
    "civalgo:migrate": "yarn civalgo:migrate-prisma && yarn db:electify && yarn client:generate",
    "reset:items": "yarn backend:down && rimraf src/generated && yarn backend:up",
    "migrate:items": "yarn db:migrate && yarn client:generate"
  },
  "dependencies": {
    "electric-sql": "^0.7.0",
    "expo": "^50.0.2",
    "expo-sqlite": "~13.2.1",
    "expo-status-bar": "~1.11.1",
    "react": "^18.2.0",
    "react-native": "0.73.2",
    "react-native-base64": "^0.2.1",
    "react-native-get-random-values": "~1.8.0",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@databases/pg-migrations": "^5.0.2",
    "@electric-sql/prisma-generator": "^1.1.0",
    "@prisma/client": "4.8.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.72.2",
    "fs-extra": "^10.0.0",
    "prisma": "4.8.1",
    "prompt": "^1.3.0",
    "shelljs": "^0.8.5",
    "tcp-port-used": "^1.0.2",
    "typescript": "^5.3.0"
  }
}